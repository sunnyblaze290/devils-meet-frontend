{
  "expo": {
    "name": "Devils Meet",
    "slug": "devilsmeet",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "scheme": "devilsmeet",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devilsmeet.app",
      "buildNumber": "37",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "We use your photo library to let you upload images to event posts and customize your profile."
      }
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devilsmeet.app"
    },
    "web": {
      "bundler": "metro",
      "output": "minimal" 
    },
    "extra": {
      "eas": {
        "projectId": "3e6e717b-d250-4110-a898-31bd4790810a"
      }
    }
  }
}
